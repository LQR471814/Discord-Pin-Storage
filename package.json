{
  "name": "discord-pin-database",
  "version": "0.1.0",
  "main": "./public/electron.js",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-coroutine": "^2.0.2",
    "react-dom": "^16.13.1",
    "react-new-window": "^0.1.2",
    "react-scripts": "3.4.1",
    "react-transition-group": "^4.4.1",
    "uniqid": "^5.2.0"
  },
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-pack": "build --em.main=public/electron.js",
    "preelectron-pack": "npm run build",
    "electron-dev": "concurrently \"SET BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "custom-env": "^2.0.1",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "typescript": "^3.9.6",
    "wait-on": "^5.0.1"
  },
  "build": {
    "appId": "appIdNonExistent",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
